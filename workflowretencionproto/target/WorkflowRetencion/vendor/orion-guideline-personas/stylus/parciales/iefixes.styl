.ie9
    .sweet-alert
        .icon
            &.error
                border none
                span
                    display none
                &:before
                    content "\f405"
                    font-family "Ionicons"
                    font-size 105px
                    relative top -35px  left -2px
            &.success
                border none
                span
                    display none
                .fix
                    display none
                .placeholder
                    border transparent
                    &:before
                        content "\f3fe"
                        font-family 'Ionicons'
                        font-size 105px
                        color $success-color
                        absolute top -25px left -3px
            
    .ui-scrollfix
        position relative
        
    .bch-empresas-nav 
      .nav-vtwo 
        .nav-pills 
          .bch-nav-dropdown 
            .subitems-menu-container 
              .cont-children-links
                border-left none
                border-left-color inherit
                border-left-width 0
                border-left-style none
                margin-left -3px
                display inline-block
              .cont-padres-links
                display inline-block
                

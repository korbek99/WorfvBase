.bch-paginador
	&__contador-paginas
		text-align right
		label
			color $color-12
			display inline-block
			font-family $regular
			font-size 12px
			margin-bottom 0
		p
			margin-bottom 0
		.ui-select-container
			display inline-block
			margin 0 10px
			min-width 70px
			max-width 70px
			width auto
			ul
				width 70px !important
				margin 0
				padding 0
				min-width 70px
				max-width 70px
				li
					width 70px !important
					.ui-select-choices-row
						a
							width 69px !important
							text-align center !important
							padding 0 !important
							line-height 220%
		.caret
			font-family 'Ionicons' !important
		& > p
			color $grey-two
			display inline-block
			font-family $regular
			font-size 12px
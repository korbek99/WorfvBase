.bch-empresas-paginador
	p
		color $color-12
	.form-group
		.ui-select-bootstrap
			.btn-default
				border-color $color-14
				.caret
					color $color-12
	label
		color $color-12
		
.pagination
	> li
		> a
			color $color-12 !important
			&:hover
				background-color $color-12 !important
				color $default-color !important
			&:focus
				background-color $default-color !important
				color $color-12 !important
		&.active
			a
				background-color transparent !important
				border-bottom-color $color-4  !important
				color $color-4  !important
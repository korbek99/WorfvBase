@font-face
	font-family 'bch-customs-q2'
	src url('fonts/bch-customs-q2.eot?qvwhnb')
	src url('fonts/bch-customs-q2.eot?qvwhnb#iefix') format('embedded-opentype'),
		url('fonts/bch-customs-q2.ttf?qvwhnb') format('truetype'),
		url('fonts/bch-customs-q2.woff?qvwhnb') format('woff'),
		url('fonts/bch-customs-q2.svg?qvwhnb#bch-customs-q2') format('svg')
	font-weight normal
	font-style normal

[class^="bch-ico-q2-"], [class*=" bch-ico-q2-"]
	font-family 'bch-customs-q2' !important
	speak none
	font-style normal
	font-weight normal
	font-variant normal
	text-transform none
	line-height 1
	-webkit-font-smoothing antialiased
	-moz-osx-font-smoothing grayscale

.bch-ico-q2-banca-privada:before
	content "\e900"

.bch-ico-q2-banchile-fondos:before
	content "\e901"

.bch-ico-q2-banchile-corredores:before
	content "\e902"

.bch-ico-q2-banchile-inversiones:before
	content "\e903"

.bch-ico-q2-credichile:before
	content "\e904"

.bch-ico-q2-TXT:before
	content "\e905"